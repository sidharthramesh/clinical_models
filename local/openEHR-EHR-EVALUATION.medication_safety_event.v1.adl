															at0330,    -- Psychotherapy required
															at0359,    -- Error corrected
															at0360,    -- Monitoring required
															at0363,    -- Patient informed
															at0331]    -- Other
														}
													}
												}
											}
											ELEMENT[at0348] occurrences matches {0..1} matches {    -- Comment
												value matches {
